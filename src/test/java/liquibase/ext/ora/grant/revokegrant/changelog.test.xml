<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:ora="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog ../../../../../liquibase/ext/ora/xml/dbchangelog-3.2.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext ../../../../../liquibase/ext/ora/xml/dbchangelog-ext.xsd">

    <changeSet id="1" author="kellerj">
        <createTable tableName="addgrant">
            <column name="id" type="integer"/>
            <column name="name" type="varchar2(50)"/>
        </createTable>
    </changeSet>


    <changeSet id="2" author="kellerj">
        <ora:grantObjectPermission objectName="addgrant" recipientList="SYSTEM" select="true" update="true" insert="true" delete="true" />
    </changeSet>

    <changeSet id="3" author="kellerj">
        <ora:revokeObjectPermission objectName="addgrant" recipientList="SYSTEM" update="true" insert="true" delete="true" />
    </changeSet>

</databaseChangeLog>